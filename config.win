# Installation directories
# System's libraries directory (where binary libraries are installed)
LUA_LIBDIR= F:\MyProject\Lua\LuaProfiler\Luaprofiler-1\lua
# Lua includes directory
LUA_INC= F:\MyProject\Lua\LuaProfiler\Luaprofiler-1\lua\include
# Lua library
LUA_LIB= F:\MyProject\Lua\LuaProfiler\Luaprofiler-1\lua\lib\release\Lua.lib

LIBNAME= $T.dll

# Compilation directives
WARN= /O2
INCS= /I$(LUA_INC)
CFLAGS= $(WARN) $(INCS)
CC= cl

